  "scripts": {
    "start": "npm run watch",
    "build": "tsc",
    "watch": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "node_modules/.bin/webpack-dev-server --mode development",
    "watch:server": "nodemon -r ts-node/register ./src_backend/server.ts"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "http": "0.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "uuid": "^3.3.3",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/uuid": "^3.4.6",
    "@types/ws": "^6.0.4",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^6.7.1",
    "eslint-plugin-import": "^2.18.2",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
